<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_chat_nav"
    app:startDestination="@id/mainHomeFragment">

    <fragment
        android:id="@+id/chatHolderFragment"
        android:name="com.example.myapplication1.view.fragments.chat_holder.ChatHolderFragment"
        android:label="fragment_chat_holder"
        tools:layout="@layout/fragment_chat_holder" >
        <action
            android:id="@+id/action_chatHolderFragment_to_chatDetailFragment"
            app:destination="@id/chatDetailFragment" />
        <action
            android:id="@+id/action_chatHolderFragment_to_contactListFragment2"
            app:destination="@id/contactListFragment" />
        <action
            android:id="@+id/action_chatHolderFragment_to_contactDetailsFragment"
            app:destination="@id/contactDetailsFragment" />
        <action
            android:id="@+id/action_chatHolderFragment_to_profileDialogFragment"
            app:destination="@id/profileDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatDetailFragment"
        android:name="com.example.myapplication1.view.fragments.chat_detail.ChatDetailFragment"
        android:label="fragment_chat_detail"
        tools:layout="@layout/fragment_chat_detail" >
        <argument
            android:name="chat_id"
            app:argType="integer" />
        <argument
            android:name="contactId"
            app:argType="integer" />
        <action
            android:id="@+id/action_chatDetailFragment_to_contactDetailsFragment"
            app:destination="@id/contactDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/contactDetailsFragment"
        android:name="com.example.myapplication1.view.fragments.contact_detail.ContactDetailsFragment"
        android:label="contact_details_fragment"
        tools:layout="@layout/contact_details_fragment" >
        <argument
            android:name="contact_id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/contactListFragment"
        android:name="com.example.myapplication1.view.fragments.contact_list.ContactListFragment"
        android:label="fragment_contact_list"
        tools:layout="@layout/fragment_contact_list" >
        <action
            android:id="@+id/action_contactListFragment_to_chatDetailFragment"
            app:destination="@id/chatDetailFragment" />
        <action
            android:id="@+id/action_contactListFragment_to_contactDetailsFragment"
            app:destination="@id/contactDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainHomeFragment"
        android:name="com.example.myapplication1.view.fragments.home.MainHomeFragment"
        android:label="fragment_home_main"
        tools:layout="@layout/fragment_home_main" >
        <action
            android:id="@+id/action_mainHomeFragment_to_chatHolderFragment"
            app:destination="@id/chatHolderFragment"
            app:popUpTo="@id/mainHomeFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_newsMainFragment"
            app:destination="@id/newsMainFragment"
            app:popUpTo="@id/mainHomeFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_drawingMain"
            app:destination="@id/drawingMain" />
        <action
            android:id="@+id/action_mainHomeFragment_to_workoutLandingPageFragment"
            app:destination="@id/workoutLandingPageFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_weatherMainFragment"
            app:destination="@id/weatherMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/workOutHomeFragment"
        android:name="com.example.myapplication1.workout.ui.home.WorkOutHomeFragment"
        android:label="fragment_work_out_home"
        tools:layout="@layout/fragment_work_out_home" >
        <action
            android:id="@+id/action_workOutHomeFragment_to_workOutDetailFragment"
            app:destination="@id/workOutDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsMainFragment"
        android:name="com.example.myapplication1.news.ui.main.NewsMainFragment"
        android:label="fragment_news_main"
        tools:layout="@layout/fragment_news_main" >
        <action
            android:id="@+id/action_newsMainFragment_to_savedNewsFragment"
            app:destination="@id/savedNewsFragment" />
        <action
            android:id="@+id/action_newsMainFragment_to_breakingNewsFragment"
            app:destination="@id/breakingNewsFragment" />
        <action
            android:id="@+id/action_newsMainFragment_to_searchNewsFragment"
            app:destination="@id/searchNewsFragment" />
        <action
            android:id="@+id/action_newsMainFragment_to_newsArticleFragment"
            app:destination="@id/newsArticleFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedNewsFragment"
        android:name="com.example.myapplication1.news.ui.fragments.saved.SavedNewsFragment"
        android:label="fragment_saved_news"
        tools:layout="@layout/fragment_saved_news" />
    <fragment
        android:id="@+id/breakingNewsFragment"
        android:name="com.example.myapplication1.news.ui.fragments.breaking.BreakingNewsFragment"
        android:label="fragment_breaking_news"
        tools:layout="@layout/fragment_breaking_news" />
    <fragment
        android:id="@+id/searchNewsFragment"
        android:name="com.example.myapplication1.news.ui.fragments.searchNews.SearchNewsFragment"
        android:label="fragment_search_news"
        tools:layout="@layout/fragment_search_news" />
    <fragment
        android:id="@+id/newsArticleFragment"
        android:name="com.example.myapplication1.news.ui.fragments.article.NewsArticleFragment"
        android:label="fragment_news_article"
        tools:layout="@layout/fragment_news_article"
        >
        <argument
            android:name="article"
            app:argType="com.example.myapplication1.news.models.Article" />
    </fragment>
    <fragment
        android:id="@+id/workOutDetailFragment"
        android:name="com.example.myapplication1.workout.ui.details.WorkOutDetailFragment"
        android:label="fragment_work_out_detail"
        tools:layout="@layout/fragment_work_out_detail" >
        <argument android:name="workout_id"
            app:argType="integer" />
    </fragment>
    <dialog
        android:id="@+id/profileDialogFragment"
        android:name="com.example.myapplication1.view.fragments.profile_dialog.ProfileDialogFragment"
        android:label="fragment_profile_dialog"
        tools:layout="@layout/fragment_profile_dialog" >
        <argument
            android:name="contact_id"
            app:argType="integer" />
        <argument
            android:name="from"
            app:argType="string" />
        <argument
            android:name="transition_name"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/drawingMain"
        android:name="com.example.myapplication1.drawing_app.ui.DrawingMain"
        android:label="fragment_drawing_main"
        tools:layout="@layout/fragment_drawing_main" />
    <fragment
        android:id="@+id/workoutLandingPageFragment"
        android:name="com.example.myapplication1.workout.ui.WorkoutLandingPageFragment"
        android:label="fragment_workour_landing_page"
        tools:layout="@layout/fragment_workour_landing_page" >
        <action
            android:id="@+id/action_workoutLandingPageFragment_to_workOutHomeFragment"
            app:destination="@id/workOutHomeFragment" />
        <action
            android:id="@+id/action_workoutLandingPageFragment_to_workoutFragment"
            app:destination="@id/workoutFragment" />
    </fragment>
    <fragment
        android:id="@+id/workoutFragment"
        android:name="com.example.myapplication1.workout.ui.workoutplan.WorkoutFragment"
        android:label="fragment_workout_list"
        tools:layout="@layout/fragment_workout_list" />
    <fragment
        android:id="@+id/weatherMainFragment"
        android:name="com.example.myapplication1.weather.views.WeatherMainFragment"
        android:label="fragment_weather_main"
        tools:layout="@layout/fragment_weather_main" />
</navigation>